Config { font = "-b&h-lucida-medium-r-normal-sans-10-100-75-75-p-58-iso8859-1"
	, bgColor = "#2f4f4f"
	, fgColor = "#ffaaaa"
	, position = Bottom
	, lowerOnStart = True
